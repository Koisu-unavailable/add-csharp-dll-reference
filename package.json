{
  "name": "add-csharp-dll-reference",
  "displayName": "Add c# dll reference",
  "description": "Add a dll reference for c sharp projects",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .dll",
          "command": "add-csharp-dll-reference.addDllReference"
        }
      ]
    },
    "commands": [
      {
        "command": "add-csharp-dll-reference.addDllReference",
        "title": "Add a dll reference"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "electron-edge-js": "^38.0.0",
    "fast-xml-parser": "^5.2.5"
  }
}
